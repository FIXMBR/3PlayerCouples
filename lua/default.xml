	<Layer
		Type="Quad"
		InitCommand="hidden,1"
		OnCommand="%function(self)
				padfgtweentime = 1;
				arrowrot1 = 0;
				arrowrot2 = 0;
				diff = tonumber(GAMESTATE:GetCurrentSteps(PLAYER_1):GetDifficulty());
				cur_leave_indicpattern = 1;
				cur_return_indicpattern = 1;

				-- Adjust the offset of arrow animations here (in seconds) --
				leave_var = 0.4;
				return_var = 0.4;
				

				------------------
				--  INDICATORS  --
				------------------

				-- diff_leave_indicactivity - enter animations with arrow pointing out of the screen (player leaves pads) --
				-- diff_return_indicactivity - enter animations with arrow pointing to the center of the screen (player returns to pads) --
				-- beat should be equal to the player's very last note (leave) or to the player's very first note (return) --
				-- leave: {beat,'xyz'} --
				-- return: {beat,'xyz',warning animation length in seconds - 0 for no animation, >=1 for animation} --

				-- NOVICE --
				novice_leave_indicactivity = {

								};

				novice_return_indicactivity = {

								};

				-- EASY --
				easy_leave_indicactivity = {

								};

				easy_return_indicactivity = {

								};

				-- MEDIUM --
				medium_leave_indicactivity = {
					-- {10,'l1r'}, --
					-- {16,'r2l'} --
								};

				medium_return_indicactivity = {
					-- {14,'r3l',2} --
								};

				-- HARD --
				hard_leave_indicactivity = {

								};

				hard_return_indicactivity = {

								};

				-- EXPERT --
				expert_leave_indicactivity = {

								};

				expert_return_indicactivity = {

								};

				-- EDIT --
				edit_leave_indicactivity = {

								};

				edit_return_indicactivity = {

								};

				-- end INDICATORS --



				function SwitchIndicator(a1,length)
					if a1 == 'r1r' or a1 == 'r2r'or a1 == 'r3r' or a1 == 'r4r' or a1 == 'd1d' or a1 == 'd2d' or a1 == 'd3d' or a1 == 'd4d' then
						_G['couples_'..a1]:decelerate(0.5);
						_G['couples_'..a1]:zoom(1.5);
						_G['couples_'..a1]:accelerate(0.5);
						_G['couples_'..a1]:addy(-100);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:zoom(0);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:addy(100);
					elseif a1 == 'l1l' or a1 == 'l2l' or a1 == 'l3l' or a1 == 'l4l' or a1 == 'u1u' or a1 == 'u2u' or a1 == 'u3u' or a1 == 'u4u' then
						_G['couples_'..a1]:decelerate(0.5);
						_G['couples_'..a1]:zoom(1.5);
						_G['couples_'..a1]:accelerate(0.5);
						_G['couples_'..a1]:addy(100);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:zoom(0);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:addy(-100);
					elseif a1 == 'l1r' or a1 == 'l2r' or a1 == 'l3r' or a1 == 'l4r' or a1 == 'u1d' or a1 == 'u2d' or a1 == 'u3d' or a1 == 'u4d' then
						if length == 0 then
						_G['couples_'..a1]:linear(0.1);
