	<Layer
		Type="Quad"
		InitCommand="hidden,1"
		OnCommand="%function(self)
				padfgtweentime = 1;
				arrowrot1 = 0;
				arrowrot2 = 0;
				diff = tonumber(GAMESTATE:GetCurrentSteps(PLAYER_1):GetDifficulty());
				
				curindicpattern = 1;

				 -- {beat,'xyz'} --

				-- NOVICE --
				noviceindicactivity = {

								};

				 -- EASY --
				easyindicactivity = {

								};

				-- MEDIUM --
				mediumindicactivity = {

								};

				-- HARD --
				hardindicactivity = {

								};

				-- EXPERT --
				expertindicactivity = {

								};

				-- EDIT --
				editindicactivity = {

								};
				
				
				function SwitchIndicator(a1)
					if a1 == 'rrr' or a1 == 'rbr'or a1 == 'ryr' or a1 == 'drd' or a1 == 'dbd' or a1 == 'dyd' then
						_G['couples_'..a1]:decelerate(0.5);
						_G['couples_'..a1]:zoom(1.5);
						_G['couples_'..a1]:accelerate(0.5);
						_G['couples_'..a1]:addy(-100);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:zoom(0);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:addy(100);
					elseif a1 == 'lrl' or a1 == 'lbl' or a1 == 'lyl' then
						_G['couples_'..a1]:decelerate(0.5);
						_G['couples_'..a1]:zoom(1.5);
						_G['couples_'..a1]:accelerate(0.5);
						_G['couples_'..a1]:addy(100);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:zoom(0);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:addy(-100);
					elseif a1 == 'lrr' or a1 == 'lbr' or a1 == 'lyr' then
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:addy(100);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:zoom(1.5);
						_G['couples_'..a1]:decelerate(0.6);
						_G['couples_'..a1]:addy(-100);
						_G['couples_'..a1]:sleep(0.2);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:zoom(0);
					elseif a1 == 'rrl' or a1 == 'rbl' or a1 == 'dbu' or a1 == 'dru'or a1 == 'ryl' or a1 == 'dyu' then
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:addy(-100);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:zoom(1.5);
						_G['couples_'..a1]:decelerate(0.6);
						_G['couples_'..a1]:addy(100);
						_G['couples_'..a1]:sleep(0.2);
						_G['couples_'..a1]:linear(0.1);
						_G['couples_'..a1]:zoom(0);
					end
				end
				
				
				self:queuecommand('Update');
			end"
		
		UpdateCommand="%function(self)
		
			--if GAMESTATE:IsCourseMode() then


			-- Choosing and performing the correct list for chart's difficulty --

			if diff == 0 then
			
				if curindicpattern <= table.getn(noviceindicactivity) and GAMESTATE:GetSongBeat()>=noviceindicactivity[curindicpattern][1] then
					SwitchIndicator(noviceindicactivity[curindicpattern][2],noviceindicactivity[curindicpattern][3],noviceindicactivity[curindicpattern][4]);
					curindicpattern = curindicpattern + 1;
				end

			elseif diff == 1 then
			
				if curindicpattern <= table.getn(easyindicactivity) and GAMESTATE:GetSongBeat()>=easyindicactivity[curindicpattern][1] then
					SwitchIndicator(easyindicactivity[curindicpattern][2],easyindicactivity[curindicpattern][3],easyindicactivity[curindicpattern][4]);
					curindicpattern = curindicpattern + 1;
				end

			elseif diff == 2 then
			
				if curindicpattern <= table.getn(mediumindicactivity) and GAMESTATE:GetSongBeat()>=mediumindicactivity[curindicpattern][1] then
					SwitchIndicator(mediumindicactivity[curindicpattern][2],mediumindicactivity[curindicpattern][3],mediumindicactivity[curindicpattern][4]);
					curindicpattern = curindicpattern + 1;
				end

			elseif diff == 3 then
			
				if curindicpattern <= table.getn(hardindicactivity) and GAMESTATE:GetSongBeat()>=hardindicactivity[curindicpattern][1] then
					SwitchIndicator(hardindicactivity[curindicpattern][2],hardindicactivity[curindicpattern][3],hardindicactivity[curindicpattern][4]);
					curindicpattern = curindicpattern + 1;
				end

			elseif diff == 4 then
			
				if curindicpattern <= table.getn(expertindicactivity) and GAMESTATE:GetSongBeat()>=expertindicactivity[curindicpattern][1] then
					SwitchIndicator(expertindicactivity[curindicpattern][2],expertindicactivity[curindicpattern][3],expertindicactivity[curindicpattern][4]);
					curindicpattern = curindicpattern + 1;
				end

			else

				if curindicpattern <= table.getn(editindicactivity) and GAMESTATE:GetSongBeat()>=editindicactivity[curindicpattern][1] then
					SwitchIndicator(editindicactivity[curindicpattern][2],editindicactivity[curindicpattern][3],editindicactivity[curindicpattern][4]);
					curindicpattern = curindicpattern + 1;
				end

			end

			-- End choosing difficulty --
				
				
				
				self:queuecommand('Update2');
			
			--end
			
		end"
  

		Update2Command="%function(self)
		self:sleep(0.1);
		self:queuecommand('Update');
		end"
		
		
		
	/>

	
</children></ActorFrame>